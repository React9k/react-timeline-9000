{
  "name": "@crispico/react-timeline-10000",
  "version": "2.2.0-SNAPSHOT3",
  "description": "Performance focused timeline for react",
  "private": false,
  "types": "./types/index.d.ts",
  "scripts": {
    "build": "yarn tsc && webpack --config webpack.prod.js",
    "build_lib": "yarn tsc && babel src --out-dir lib -x '.js,.jsx,.mdx,.html,.tsx' --ignore **/demo.js,**/demo_index.js,**/demo.html,**/demo/,**/setupTests.js,**/*/*.test.js,**/stories/,**/testsAreDemo --copy-files --source-maps inline --no-copy-ignored",
    "build_demo": "webpack --config webpack.demo.prod.js",
    "start": "webpack-dev-server --open --config webpack.dev.js",
    "docs": "esdoc && mv docs/index.html docs/index-docs.html",
    "pretty": "prettier --write --tab-width 4 \"src/**/*.js\"",
    "precommit": "lint-staged",
    "storybook": "yarn tsc && start-storybook -p 6006",
    "storybook-build": "yarn tsc && yarn docs && build-storybook",
    "storybook-move": "rm -r ../flower-platform.github.io/foundation-react-gantt; mv storybook-static ../flower-platform.github.io/foundation-react-gantt",
    "storybook-build-move": "yarn storybook-build && yarn storybook-move",
    "tsc": "tsc -p tsconfig.ts-types-from-jsdoc.json && cp src/types.d.ts types/types.d.ts"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "keywords": [
    "react",
    "timeline"
  ],
  "engines": {
    "node": ">=4.2.4"
  },
  "author": "Leighton Lilford",
  "license": "MIT",
  "files": [
    "lib",
    "types"
  ],
  "main": "lib/index.js",
  "homepage": "https://github.com/flower-platform/react-timeline-10000",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@famiprog-foundation/tests-are-demo": "^0.8.0",
    "@storybook/addon-actions": "^6.3.11",
    "@storybook/addon-essentials": "^6.3.11",
    "@storybook/addon-links": "^6.3.11",
    "@storybook/addon-storysource": "^6.5.12",
    "@storybook/react": "^6.3.11",
    "antd": "^3.6.5",
    "babel-loader": "^8.2.5",
    "chai": "^4.1.2",
    "core-js": "^2.4.0",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "esdoc": "^1.1.0",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-inject-style-plugin": "^1.0.0",
    "esdoc-jsx-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^5.0.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.10.0",
    "html-webpack-plugin": "5.5.0",
    "husky": "^0.14.3",
    "ignore-styles": "^5.0.1",
    "jsdom": "^16.5.0",
    "lint-staged": "8.2.1",
    "prettier": "^1.13.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "semantic-ui-css": "^2.5.0",
    "semantic-ui-react": "^2.1.3",
    "style-loader": "^0.21.0",
    "tern": "^0.21.0",
    "ts-loader": "^9.4.2",
    "typescript": "~4.4.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^5.67.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.3",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.21.0",
    "color": "^4.2.3",
    "core-js": "^2.4.0",
    "interactjs": "^1.6.2",
    "lodash": "^4.17.19",
    "moment": "^2.22.2",
    "pretty-format": "^29.5.0",
    "react-measure": "^2.5.2",
    "react-virtualized": "^9.19.1"
  },
  "dependencies / comments": {
    "// pretty-format": "We have a TypeError on buid, it is imported from @testing-library/dom but was needed to add it explicitly"
  },
  "peerDependencies": {
    "react": "^16.4.1 || ^17.0.0",
    "react-dom": "^16.4.1 || ^17.0.0"
  }
}
